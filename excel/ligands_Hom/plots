> png('hom_GRE_6.0_SAM_CA_ACC_DON_beeswarm.png')
> beeswarm(auc ~ class, singcomb, col=sample(colors(),10), pch=19, method="swarm",cex=0.5,main="hom_GRE_6.0_LLP_CA_ACC_DON_beeswarm(Srel)")

U5P

> singcomb <- subset(sing, sing$atomtype=="CA" & sing$cuttoff==2.5 & sing$clique==5 & sing$distance==6.0 & sing$metric=="SO", select=c(code,class,auc))
> png('hom_GRE_6.0_U5P_CA_beeswarm.png')
> beeswarm(auc ~ class, singcomb, col=sample(colors(),10), pch=19, method="swarm",cex=0.5,main="hom_GRE_6.0_U5P_CA_beeswarm(SO)")
> dev.off()

> png('hom_GRE_6.0_U5P_CA_boxplot.png')
> boxplot(auc ~ class, singcomb, xlab = "Ligand", ylab="AUC",main="hom_GRE_6.0_U5P_CA_boxplot(SO)")
> dev.off()

