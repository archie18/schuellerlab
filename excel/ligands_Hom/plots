
SAM
> singcomb <- subset(sing, sing$atomtype=="CA" & sing$cuttoff==3.5 & sing$clique==5 & sing$distance==6.0 & sing$metric=="SrelScore08", select=c(code,class,auc))

> png('hom_GRE_6.0_SAM_CA_boxplot.png')
> boxplot(auc ~ class, singcomb, xlab = "Ligand", ylab="AUC",main="hom_GRE_6.0_SAM_CA_boxplot(SrelScore08)")
> dev.off()
> png('hom_GRE_6.0_SAM_CA_beeswarm.png')
> beeswarm(auc ~ class, singcomb, col=sample(colors(),10), pch=19, method="swarm",cex=0.5,main="hom_GRE_6.0_SAM_CA_beeswarm(SrelScore08)")
> dev.off()



